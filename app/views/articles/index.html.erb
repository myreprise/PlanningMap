<div class="container">
	<div class="row">
		<div class="col-md-9">
			<%= render 'articles' %>
			<%= will_paginate @articles %>
		</div>
		<div class="col-md-3">
			<%= form_tag articles_path, :method => 'get' do %>
			  <p>
			    <%= text_field_tag :search, params[:search] %>
			    <%= submit_tag "Search", :name => nil %>
			  </p>
			<% end %>
		</div> <!-- end of the col-md -->
	</div> <!-- end of the row -->
</div> <!-- end of container -->