    $("#search").keyup(function() {
        var value = this.value.toLowerCase().trim();
        $("#project_list").find(".entry").each(function(index) {
            var id = $(this).find(".searchable").text().toLowerCase().trim();
            $(this).toggle(id.indexOf(value) !== -1);
        });

    });
