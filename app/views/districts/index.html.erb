<div class="container">
  <div class="row">
    <div class="col-md-12">

<h1>Listing <%= @city.name %> Districts</h1>
<%= link_to 'New District', new_city_district_path %>

<table class="table table-hover">
  <thead>
    <tr>
            <th>District</th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= link_to district.name, city_district_path(@city, district) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

    </div> <!-- end col-md -->
  </div> <!-- end row -->
    <div class="row">
      <div class="col-md-12">
      
        <div id="allmap"></div>
      
      </div> <!-- end col-md -->

    </div>
</div> <!-- end container -->

    <script type="text/javascript" src="http://api.map.baidu.com/api?v=2.0&ak=BF934346e7e1c13620f7d9de908ed808"></script>

    <script type="text/javascript">
      createMap(<%= @city.longitude %>, <%= @city.lattitude %>, 11, gon.projects);
    </script>